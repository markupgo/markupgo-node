    "build": "tsup",
    "publish-to-npm": "npm run build && npm publish",
    "typecheck": "tsc --noemit",
    "lint": "eslint --ignore-path .gitignore --ext .ts src --fix && npm run typecheck",
    "start:dev": "tsx watch --tsconfig tsconfig.json src/playground/index.ts"
